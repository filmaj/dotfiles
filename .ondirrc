enter ~/src/sauce
  workon sauce

enter ~/src/cloud
  workon cloud

enter ~/src/sauce-ansible
  workon sauce-ansible
  git submodule update --init

enter ~/src/changelog
  workon changelog

enter ~/src/gitdumper
  workon gitdumper

enter ~/src/challenges
  workon challenges

enter ~/src/python-selenium-tests
  workon python-selenium-tests

enter ~/src/firehose
  workon firehose

leave ~/src/sauce:~/src/sauce-ansible:~/src/changelog:~/src/challenges:~/src/python-selenium-tests:~/src/gitdumper:~/src/cloud:~/src/firehose
  deactivate > /dev/null 2>&1
