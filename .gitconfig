[user]
    name = filmaj
    email = maj.fil@gmail.com
[alias]
    lawg = log --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit --date=relative
    whatsnew = diff HEAD@{1} HEAD
[color]
    ui = true
[push]
    default = simple
[core]
    editor = /usr/bin/vim
	pager = diff-so-fancy | less --tabs=4 -RFX
[diff]
    compactionHeuristic = true
[credential]
	helper = osxkeychain
[help]
	autocorrect = 1
[pager]
	branch = false
