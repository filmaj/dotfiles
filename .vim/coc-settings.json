{
  "coc.preferences.rootPatterns": [
    "package.json",
    ".git",
    ".hg",
    ".projections.json"
  ],
  "suggest.defaultSortMethod": "none",
  "suggest.maxCompleteItemCount": 50,
  "suggest.triggerCompletionWait": 25,
  "suggest.lowPrioritySourceLimit": 5,
  "suggest.removeDuplicateItems": true,
  "suggest.localityBonus": false,
  "suggest.labelMaxLength": 125,
  "suggest.detailMaxLength": 100,
  "eslint.alwaysShowStatus": true,
  "eslint.run": "onSave",
  "python.formatting.provider": "black",
  "python.linting.flake8Enabled": true
}
